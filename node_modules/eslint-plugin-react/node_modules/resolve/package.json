  },
  "dependencies": {
    "is-core-module": "^2.13.0",
    "path-parse": "^1.0.7",
    "supports-preserve-symlinks-flag": "^1.0.0"
  },
  "publishConfig": {
    "ignore": [
      ".github/workflows",
      "appveyor.yml",
      "test/resolver/malformed_package_json"
    ]
  },
